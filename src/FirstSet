<range> = {NUM}
<iterativeStmt> = {FOR,WHILE}
<default> = {DEFAULT} U {e}
<value> = {NUM} U {TRUE} U {FALSE}
<caseStmt> = {CASE}
<conditionalStmt> = {SWITCH}
<declare> = {DECLARE}
<relationalOP> = {LT} U {LE} U {GT} U {GE} U {EQ} U {NE}
//// <booleanExp> = {BO} U {BO,ID,NUM,RNUM}
<logicalOp> = {AND,OR}
<op>  = {PLUS,MINUS,MUL,DIV}
<factor> = {BO}U{ID,NUM,RNUM}
<var>  = {ID,NUM,RNUM}
<term> = {BO,ID,NUM,RNUM}
<arithmeticExpr> = {BO,ID,NUM,RNUM}
//// <expression>  = {BO,ID,NUM,RNUM} U {BO,ID,NUM,RNUM}
<idList> = {ID}
<optional> = {SQBO} U {e}
<moduleReuseStmt>  = {SQBO,USE,e}
<index> = {NUM,ID}
<lvalueARRStmt> = {SQBO}
<lvalueIDStmt> = {ASSIGNOP}
<whichStmt>  = {SQBO} U {ASSIGNOP}
<assignmentStmt> = {ID}
<simpleStmt> = {ID} U {SQBO,USE,e}
<whichId> = {SQBO,e}
<ioStmt> = {GET_VALUE, PRINT}
<statement>  = {GET_VALUE,PRINT} U {ID,SQBO,USE,e} U {DECLARE} U  {SWITCH} U {FOR,WHILE}
///////<statements> = {GET_VALUE,PRINT,ID,SQBO,USE,e} U {e}
<moduleDef>  = {START}
<type> = {INTEGER,REAL,BOOLEAN}
<dataType> = {INTEGER,REAL,BOOLEAN,ARRAY}
<inputPlist> = {ID}
<outputPlist> = {ID}
<ret>  = {RETURNS,e}
<module>  = {DEF}
<driverModule = {DEF}
<otherModules>  = {DEF} U {e}
<moduleDeclaration>  = {DECLARE}
<moduleDeclarations>  = {DECLARE}U{e}
<program>  = {DECLARE,DEF,e}
